  meta(name='description', content='')
  meta(http-equiv='X-UA-Compatible', content='IE=edge')
  meta(name='viewport', content='width=device-width, initial-scale=1, shrink-to-fit=no')
  // Title
  title Razo - Radio HTML Template | Home
  // Favicon
  link(rel='icon', href='./img/core-img/favicon.ico')
  // Stylesheet
  link(rel='stylesheet', href='style.css')
// Preloader
#preloader
  div
    .spinner
      .double-bounce1
      .double-bounce2
    span Wait, please...
// /Preloader
// Top Search Area Start
.top-search-area
  #searchModal.modal.fade(tabindex='-1', role='dialog', aria-hidden='true')
    .modal-dialog.modal-dialog-centered(role='document')
      .modal-content
        .modal-body
          // Close Button
          button.btn.close-btn(type='button', data-dismiss='modal')
            i.fa.fa-times
          // Form
          form(action='index.html', method='post')
            input.form-control(type='search', name='top-search-bar', placeholder='Type keywords and hit enter...')
            button(type='submit') Search
          // Search Button
          .search-btn
            i.icon_search
// Top Search Area End
// Social Share Area Start
.razo-social-share-area
  a.facebook(href='#')
    i.fa.fa-facebook
  a.twitter(href='#')
    i.fa.fa-twitter
  a.pinterest(href='#')
    i.fa.fa-pinterest
  a.instagram(href='#')
    i.fa.fa-instagram
  a.youtube(href='#')
    i.fa.fa-youtube-play
  a.ss-close-btn(href='#')
